  <entry>
    <title>基于 postcss 插件实现项目 vw 配置</title>
    <link href="http://example.com/2023/12/07/%E5%9F%BA%E4%BA%8Epostcss%E6%8F%92%E4%BB%B6%E5%AE%9E%E7%8E%B0%E9%A1%B9%E7%9B%AEvw%E9%85%8D%E7%BD%AE/"/>
